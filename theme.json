{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "list.dropBackground": "#3A3934",
		"editor.foreground": "#D0D0D0",
		"editor.background": "#2B2A26",
		"sideBar.background": "#262622",
		"activityBar.background": "#20201D",
		"editor.lineHighlightBackground": "#34332E",
		"editor.inactiveSelectionBackground": "#6A6A5A2A",
		"editor.selectionHighlightBackground": "#7A7A6655",
		"editor.wordHighlightBackground": "#6A6A5A36",
		"editor.wordHighlightStrongBackground": "#6A6A5A66",
		"editorCursor.foreground": "#D8B56A",
		"editorIndentGuide.background1": "#3F3E3A",
		"editorBracketHighlight.foreground1": "#E2C27A",
		"editorBracketHighlight.foreground2": "#D2B36F",
		"editorBracketHighlight.foreground3": "#C2A565",
		"editorBracketHighlight.foreground4": "#B2965B",
		"editorBracketHighlight.foreground5": "#A28752",
		"editorBracketHighlight.foreground6": "#92794A",
		"editorBracketPairGuide.background1": "#3F3E3A",
		"editorBracketPairGuide.background2": "#3A3934",
		"editorBracketPairGuide.background3": "#35342F",
		"editorStickyScroll.background": "#262622"
  },

  /* ============================================================
     TEXTMATE TOKENS — NON-GO LANGUAGES (ALL PRESERVED)
     ============================================================ */
  "tokenColors": [
    /* COMMENTS */
    {
      "scope": "comment",
      "settings": {
        "foreground": "#7A7A6A",
        "fontStyle": "italic"
      }
    },

    /* STRINGS & NUMBERS */
    {
      "scope": ["string", "string.value"],
      "settings": { "foreground": "#6A8759" }
    },
    {
      "scope": "constant.numeric",
      "settings": { "foreground": "#6897BB" }
    },

    /* KEYWORDS */
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },

    /* FUNCTIONS */
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#D3AC6C" }
    },

    /* USER TYPES (classes, interfaces) */
    {
      "scope": ["entity.name.type", "support.class"],
      "settings": { "foreground": "#B88DAC" }
    },

    /* GENERIC / BUILTIN TYPES */
    {
      "scope": "support.type",
      "settings": { "foreground": "#A87E8F" }
    },

    /* VARIABLES */
    {
      "scope": ["variable.other", "variable.parameter"],
      "settings": { "foreground": "#9AACBD" }
    },

    /* ================= JAVA ================= */

    {
      "scope": [
        "storage.type.primitive.java",
        "storage.type.void.java"
      ],
      "settings": { "foreground": "#A87E8F" }
    },
    {
      "scope": "support.type.java",
      "settings": { "foreground": "#B88DAC" }
    },
    {
      "scope": "meta.annotation.java",
      "settings": {
        "foreground": "#BBB529",
        "fontStyle": "italic"
      }
    },

    /* ================= TYPESCRIPT / JAVASCRIPT ================= */

    {
      "scope": [
        "support.type.primitive.ts",
        "support.type.primitive.js"
      ],
      "settings": { "foreground": "#A87E8F" }
    },
    {
      "scope": [
        "entity.name.type.ts",
        "entity.name.type.js"
      ],
      "settings": { "foreground": "#B88DAC" }
    },
    {
      "scope": [
        "keyword.declaration.ts",
        "keyword.declaration.js"
      ],
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.decorator.ts",
      "settings": {
        "foreground": "#BBB529",
        "fontStyle": "italic"
      }
    },

    /* ================= PYTHON ================= */

    {
      "scope": [
        "storage.type.function.python",
        "storage.type.class.python"
      ],
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.type.builtin.python",
      "settings": { "foreground": "#A87E8F" }
    },
    {
      "scope": "entity.name.type.class.python",
      "settings": { "foreground": "#B88DAC" }
    },
    {
      "scope": "meta.decorator.python",
      "settings": {
        "foreground": "#BBB529",
        "fontStyle": "italic"
      }
    }
  ],

  /* ============================================================
     SEMANTIC TOKENS — GO LANGUAGE (COMPLETE & DISTINCT)
     ============================================================ */
  "semanticTokenColors": {
    /* ========== VARIABLES ========== */
    
    /* Regular variables - Most common, gentle blue-grey */
    "variable": { 
      "foreground": "#9AACBD"
    },
    
    /* Constants - STANDS OUT STRONGLY with crimson-red */
    "variable.readonly": {
      "foreground": "#CC8866",
      "fontStyle": "bold"
    },

    /* Readonly variables (different from const) - Softer coral-red */
    "variable.readonly:go": {
      "foreground": "#C8896E",
      "fontStyle": "bold"
    },

    /* Default library variables - Softer blue */
    "variable.defaultLibrary": {
      "foreground": "#8AA0B8"
    },

    /* ========== PROPERTIES & FIELDS ========== */
    
    /* Struct fields/properties - Unified soft lavender (same for both name and value context) */
    "property": { 
      "foreground": "#A89BB9"
    },

    /* Readonly properties - Same color, just bold */
    "property.readonly": {
      "foreground": "#A89BB9",
      "fontStyle": "bold"
    },

    /* ========== FUNCTIONS & METHODS ========== */
    
    /* Functions - Classic Darcula golden yellow */
    "function": { 
      "foreground": "#D3AC6C"
    },

    /* Methods - Slightly warmer gold */
    "method": { 
      "foreground": "#D8B574"
    },

    /* Default library functions - Cooler yellow-beige */
    "function.defaultLibrary": {
      "foreground": "#C9A668"
    },

    /* ========== TYPES ========== */
    
    /* User-defined types/structs - STANDS OUT with muted orchid */
    "type": { 
      "foreground": "#B895A8"
    },

    /* Exported types - STANDS OUT MORE with brighter orchid */
    "type.exported": { 
      "foreground": "#C8A5B8"
    },

    /* Type aliases - Warmer mauve */
    "type.alias": {
      "foreground": "#B89FA8"
    },

    /* Built-in types - Dusty rose-brown */
    "type.builtin": {
      "foreground": "#A88490"
    },

    /* Default library types - Cool mauve-grey */
    "type.defaultLibrary": {
      "foreground": "#A890A0"
    },

    /* ========== INTERFACES ========== */
    
    /* Interfaces - STANDS OUT with soft periwinkle */
    "interface": {
      "foreground": "#A0A8C8"
    },

    /* Exported interfaces - Brighter periwinkle */
    "interface.exported": {
      "foreground": "#B0B8D8"
    },

    /* ========== ENUMS ========== */
    
    /* Enums - STANDS OUT with warm terracotta */
    "enum": {
      "foreground": "#C89878"
    },

    /* Enum members - STANDS OUT with peachy-rose */
    "enumMember": {
      "foreground": "#D8A888"
    },

    /* ========== NAMESPACES/PACKAGES ========== */
    
    /* Standard library packages - STANDS OUT with classic teal */
    "namespace": { 
      "foreground": "#78B8B0"
    },

    /* Third-party packages - STANDS OUT with sage green */
    "namespace.library": { 
      "foreground": "#7AAA92"
    },

    /* Exported namespaces - Brighter teal-cyan */
    "namespace.exported": {
      "foreground": "#88C8C0"
    },

    /* ========== KEYWORDS ========== */
    
    /* Keywords - Classic Darcula orange (STANDS OUT) */
    "keyword": {
      "foreground": "#CC7832",
      "fontStyle": "bold"
    },

    /* Control flow keywords - Slightly brighter orange */
    "keyword.control": {
      "foreground": "#D88040",
      "fontStyle": "bold"
    },

    /* Modifier keywords - Warmer brown-orange */
    "keyword.modifier": {
      "foreground": "#C87840",
      "fontStyle": "bold"
    },

    /* Operator keywords - Copper-orange */
    "keyword.operator": {
      "foreground": "#BC7038",
      "fontStyle": "bold"
    },

    /* ========== CONSTANTS ========== */
    
    /* Boolean/null/nil - STANDS OUT VERY STRONGLY with bright rose-red */
    "constant.language": {
      "foreground": "#C45A5A",
      "fontStyle": "bold italic"
    },

    /* Numeric constants - Blue-teal */
    "constant.numeric": {
      "foreground": "#6897BB"
    },

    /* Character constants - Olive-gold */
    "constant.character": {
      "foreground": "#9A9850"
    },

    /* ========== PARAMETERS ========== */
    
    /* Function parameters - Soft periwinkle-grey */
    "parameter": {
      "foreground": "#A8B4C8"
    },

    /* Parameter declarations - Slightly brighter */
    "parameter.declaration": {
      "foreground": "#B0BCD0"
    },

    /* ========== TYPE PARAMETERS ========== */
    
    /* Generic type parameters - STANDS OUT with sandy-gold */
    "typeParameter": {
      "foreground": "#C8B088"
    },

    /* Type parameter declarations - Warmer sand */
    "typeParameter.declaration": {
      "foreground": "#D0B890"
    },

    /* ========== LABELS ========== */
    
    /* Labels (goto labels) - Muted coral */
    "label": {
      "foreground": "#C88870"
    },

    /* ========== OPERATORS ========== */
    
    /* Operators - Light grey-blue */
    "operator": {
      "foreground": "#A0A8B0"
    },

    /* ========== STRINGS & ESCAPES ========== */
    
    /* String literals - Classic green */
    "string": {
      "foreground": "#6A8759"
    },

    /* Escape sequences in strings - Bright lime-yellow */
    "string.escape": {
      "foreground": "#A8B858"
    },

    /* ========== NUMBERS ========== */
    
    /* Numeric literals - Blue-grey */
    "number": {
      "foreground": "#6897BB"
    },

    /* ========== REGEXP ========== */
    
    /* Regular expressions - Olive-green */
    "regexp": {
      "foreground": "#88AA68"
    },

    /* ========== COMMENTS (semantic) ========== */
    
    /* Documentation comments - Greenish-grey */
    "comment.documentation": {
      "foreground": "#7A8A6A",
      "fontStyle": "italic"
    },

    /* ========== MACROS ========== */
    
    /* Macros/build tags - Yellow-ochre */
    "macro": {
      "foreground": "#C8B850"
    },

    /* ========== DECORATORS ========== */
    
    /* Decorators/annotations - Warm yellow */
    "decorator": {
      "foreground": "#BBB529",
      "fontStyle": "italic"
    },

    /* ========== MODULES ========== */
    
    /* Module declarations - Teal-blue */
    "module": {
      "foreground": "#70A8B8"
    },

    /* ========== CLASSES (if applicable) ========== */
    
    /* Class names - Soft pink-purple */
    "class": {
      "foreground": "#B890B0"
    },

    /* Exported classes - Brighter pink */
    "class.exported": {
      "foreground": "#C8A0C0"
    },

    /* ========== STRUCTS (explicit) ========== */
    
    /* Struct keyword usage - Rose-mauve */
    "struct": {
      "foreground": "#B89098"
    },

    /* ========== EVENTS ========== */
    
    /* Event handlers - Peachy-orange */
    "event": {
      "foreground": "#D8A870"
    },

    /* ========== MISC ========== */
    
    /* Punctuation - Subtle grey */
    "punctuation": {
      "foreground": "#8A8A7A"
    },

    /* Brackets - Slightly brighter grey */
    "bracket": {
      "foreground": "#9A9A8A"
    },

    /* Delimiters - Medium grey */
    "delimiter": {
      "foreground": "#8A8A80"
    }
  }
}